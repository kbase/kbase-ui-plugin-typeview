    },
    "devDependencies": {
        "bluebird": "3.7.2",
        "bower-installer": "1.3.6",
        "chalk": "5.0.1",
        "fs-extra": "10.1.0",
        "glob": "8.0.3",
        "grunt": "1.5.3",
        "grunt-contrib-copy": "1.0.0",
        "grunt-contrib-clean": "2.0.1",
        "grunt-cli": "1.4.3",
        "js-yaml": "4.1.0",
        "numeral": "2.0.6",
        "tar": "6.1.11",
